<!DOCTYPE html>
<html>
  <head>
      <% include ..\views\partials\head %>
  </head>
  <body>
    
      <header>
          <% include ..\views\partials\header %>
      </header>
   
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item"><a href="/mens">Mens</a></li>
        <li class="breadcrumb-item"><a href="/mens/mens-clothing">Clothing</a></li>
        <li class="breadcrumb-item active">Jackets</li>
      </ol>
    </br>
  </br>
</br>


     
<div class="container-name">
  <ol>
      <% _.each(items, function(things) { %>
         
          <%  if (things.primary_category_id ==="mens-clothing-jackets") { %>
            <div class="jumbotron">
            
                <span>
          <h4><%= things.page_title %></h4>
         
          <img src="/images/<%= things.image_groups[2].images[0].link %>">
                </span>
          <div style="width: 300px;">
          <p><%= things.page_description %></p>
          <p> Price:$<%=  things.price %> </p>
        
              
          
          
          <button type="button" class="btn btn-info btn-sm"> <a href="http://localhost:3000/p/<%= things.id %>"> More info</a></button>
          </div>
            
      
        </div>

      <% } %>
      <% }); %>      
              
  </ol>
</div> 




  </br>


      <footer>
        <% include ..\views\partials\footer %>
      </footer>
  
  </body>
    
  </html>
