<!DOCTYPE html>
<html>
  <head>
      <% include ..\views\partials\head %>
  </head>
  <body>
    
      <header>
          <% include ..\views\partials\header %>
      </header>
   

    </br>
  </br>
</br>


     
      <div class="container-name">
        <ol>
            <% _.each(items, function(things) { %>
               
                <%  if (things.primary_category_id ==="mens-clothing-suits") { %>
                  <div class="jumbotron">
                  <div class="col-md-12">
                  
                <h4><%= things.page_title %></h4>
               <div class="col-md-6">
                <img src="/images/<%= things.image_groups[2].images[0].link %>">
              </div>
                <div class="col-md-6">
                <p><%= things.page_description %></p>
                <p> Price:$<%=  things.price %> </p>
              
                    
                
                
                <button type="button" class="btn btn-info btn-sm"> <a href=""> More info</a></button>
                </div>
              </div>
            </div>
          
            <% } %>
            <% }); %>      
                    
        </ol>
    </div> 



  </br>


      <footer>
        <% include ..\views\partials\footer %>
      </footer>
  
  </body>
    
  </html>
